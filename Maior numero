#include <stdio.h>
#include <stdlib.h>

int main() {
    int a;
    int b;

    printf("Digite o primeiro valor: ");
    scanf("%d", &a);

    printf("Digite o segundo valor: ");
    scanf("%d", &b);

    if (a > b) {
        printf("O maior número é: %d\n", a);
    } else {
        printf("O maior número é: %d\n", b);
    }

    printf("Os dois valores são: %d, %d\n", a, b);

    return 0;
