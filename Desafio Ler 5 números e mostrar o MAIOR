#include <stdio.h>
#include <stdlib.h>
			    
int main(){
int n;
int maior=0;
int numeros[5];
int i;
			    
for(i=0;i<5;i++) {
	printf("Digite um valor:");
	scanf("%d",&numeros[i]);
}
			     
maior = numeros[0];
			    
for(i = 1; i < 5; i++) {
	if(numeros[i] > maior) {
	maior = numeros[i];
}
}
	printf("O maior numero Ã© :%d",maior);
	return 0;
}
